[32m+[m[32mpackage com.mycompany.pacman;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.File;[m[41m[m
[32m+[m[32mimport java.io.FileNotFoundException;[m[41m[m
[32m+[m[32mimport java.util.ArrayList;[m[41m[m
