[32m+[m[32m    public void generateMap() {[m
[32m+[m[32m        for(String[] sa: ms.getMapData()){[m
[32m+[m[32m            for(int i=0;i<sa.length; i++){[m
[32m+[m[32m                switch(sa[i]){[m
[32m+[m[32m                    case "K": // kant[m
