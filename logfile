[32m+[m[32m                    case "S": // sp√∏kelse spawn[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "U": // d√∏r sp√∏kelse spawn[m[41m [m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "B": // sp√∏kelse bur[m
[32m+[m[32m                    default:[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
