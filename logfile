[32m+[m[32m                    case "A": // mat for pacman[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "C": // "chase ability" for pacman[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "Y": // ingen mat[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "X": // tomt[m
